IF EXIST "%25programfiles%25\7-zip\7z.exe" GOTO st2plug
goto end

:st2plug
del "$(TargetDir)%25st2plgFile%25"
"%25programfiles%25\7-zip\7z" a -r -tzip "$(TargetDir)%25st2plgFile%25"  "$(TargetDir)*"  -x!*.st2plugin -x!*.tmp -x!*.pdb -x!$(ProjectName).xml

if "%25st2plgoutdir%25"=="" GOTO END
